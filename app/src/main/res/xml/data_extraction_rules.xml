<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Back up the DataStore preferences file -->
        <include domain="file" path="datastore/settings.preferences_pb" />
    </cloud-backup>
    <device-transfer>
        <!-- The device-transfer section is required on API 31+ -->
        <!-- All domains are included by default, so we don't need to add any rules here -->
        <!-- if we want to back up everything. -->
    </device-transfer>
</data-extraction-rules>
